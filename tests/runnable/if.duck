extern function operator>(float, float) -> float;
extern function operator+(float, float) -> float;
extern function assertEquals(float, float);

a: float = 2;
b: float = 3;
c: float = a + 2;

if a > b
  c = 1;
else
  c = 2;

assertEquals(c, 2);

if b > a
  c = 3;
else
  c = 4;

assertEquals(c, 3);

if a > 100 {
  c = 5;
}

assertEquals(c, 3);

if a > 1 {
  c = 6;
}

assertEquals(c, 6);


