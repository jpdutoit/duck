import "./assert";

extern struct duration;

extern struct Now;
extern now: Now;

extern function operator+(float, float) -> float;

extern function samples(float) -> duration;

module A {
  output: mono = 10;
}

A >> Assert(10);
A >> Assert(20);


module B {
  output: mono = 0;
  function tick() {
    output = output + 1;
  }
}

B >> Assert([1,2,3,4,5]);
B >> Assert([1,2,3,4,0]);

now += 10 samples;

