extern struct void;
extern function assertEquals(float, float);

arr: float[4];

arr[0] = 2;
arr[1] = 3;
arr[2] = 4;
arr[3] = 5;

assertEquals(arr[0], 2);
assertEquals(arr[1], 3);
assertEquals(arr[2], 4);
assertEquals(arr[3], 5);
assertEquals(arr.size, 4);

a: float[] = [1.0, 2, 3];
b: float[3]; //= [4.0, 5, 6];

assertEquals(a[0], 1);
assertEquals(a[1], 2);
assertEquals(a[2], 3);
assertEquals(a.size, 3);

//assertEquals(b[0], 4);
//assertEquals(b[1], 5);
//assertEquals(b[2], 6);
assertEquals(b.size, 3);
